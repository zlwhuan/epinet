type EdrMedicationInfo {
    id: ID!
    mpiId: String
    drugCode: String
    drugDoseCode: String
    drugDosageCode: String
    drugFrequency: String
    drugDosageRouteCode: String
    drugGivenQuantity: String
    drugBeginDate: String
    drugEndDate: String
    createOrg: String
    updateOrg: String
    createUser: String
    createTime: String
    updateUser: String
    updateTime: String
